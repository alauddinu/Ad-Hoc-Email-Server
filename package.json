{
  "name": "ahem-server",
  "version": "1.0.0",
  "license": "LGPL-3",
  "scripts": {
    "ng": "ng",
    "start": "concurrently --kill-others \"node ahem.js\" \"ng serve --proxy-config proxy.conf.json\"",
    "test": "ng test",
    "testServer": "node_modules/jest/bin/jest.js **/*/serverapp.spec.js --runInBand --detectOpenHandles --forceExit --coverage",
    "code-coverage-badges": "node_modules/jest-coverage-badges/cli.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "npm install && npm rebuild node-sass && ng build",
    "buildDev": "npm install && npm rebuild node-sass && ng build --dev",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/universalServer --proxy-config proxy.conf.json",
    "build:client-and-server-bundles": "ng build --prod && ng run ahem-universal:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors --display-error-details",
    "webpack:backend": "webpack --config webpack.backend.config.js --progress --colors --display-error-details"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/server/app/*.js"
    ],
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "coverageDirectory": "./coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.1",
    "@angular/cdk": "^9.2.0",
    "@angular/cli": "^9.1.1",
    "@angular/common": "^9.1.1",
    "@angular/compiler": "^9.1.1",
    "@angular/core": "^9.1.1",
    "@angular/forms": "^9.1.1",
    "@angular/material": "^9.2.0",
    "@angular/platform-browser": "^9.1.1",
    "@angular/platform-browser-dynamic": "^9.1.1",
    "@angular/platform-server": "^9.1.1",
    "@angular/router": "^9.1.1",
    "@fortawesome/angular-fontawesome": "^0.6.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@nguniversal/express-engine": "^9.1.0",
    "@nguniversal/module-map-ngfactory-loader": "^8.2.6",
    "@types/socket.io": "^2.1.4",
    "ajv": "^6.12.0",
    "angular-swagger-ui": "^0.6.2",
    "angulartics2": "^8.3.0",
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mailparser": "^2.7.7",
    "moment": "^2.24.0",
    "mongodb": "^3.5.5",
    "morgan": "^1.10.0",
    "ng2-adsense": "^7.0.0",
    "ngx-highlight-js": "^9.0.0",
    "ngx-moment": "^3.5.0",
    "nodemon": "^2.0.3",
    "rxjs": "^6.5.5",
    "smtp-server": "^3.6.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "swagger-ui-express": "^4.1.4",
    "ts-loader": "^6.2.2",
    "tslib": "^1.11.1",
    "winston": "^3.2.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.1",
    "@angular/compiler-cli": "^9.1.1",
    "@angular/language-service": "^9.1.1",
    "@types/jasmine": "^3.5.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^13.11.1",
    "codelyzer": "^5.2.2",
    "jasmine": "^3.5.0",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^5.0.1",
    "jasmine-ts": "^0.3.0",
    "jest": "^25.3.0",
    "jest-coverage-badges": "^1.1.2",
    "karma": "^5.0.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "mongodb-memory-server": "^6.5.2",
    "node-sass": "^4.13.1",
    "nodemailer": "^6.4.6",
    "nyc": "^15.0.1",
    "protractor": "^5.4.3",
    "supertest": "^4.0.2",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3",
    "webpack-cli": "^3.3.11"
  }
}
